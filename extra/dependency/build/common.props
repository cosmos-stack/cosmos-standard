<Project>

	<Import Project="..\..\..\build\version.props" />

	<PropertyGroup Label="Build">
		<TargetFrameworks>netstandard2.0;netstandard2.1</TargetFrameworks>
	</PropertyGroup>

	<PropertyGroup Label="Package">
		<Product>Cosmos.Extensions</Product>
		<Authors>AlexLEWIS;dotnetcore;StarrySky;Cosmos</Authors>
		<RepositoryUrl>https://github.com/cosmos-loops/Standard</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<PackageProjectUrl>https://github.com/cosmos-loops/Standard/tree/master/extra/dependency</PackageProjectUrl>
		<PackageIcon>icon.png</PackageIcon>
		<PackageLicenseFile>LICENSE</PackageLicenseFile>
		<PackageTags>StarrySky;Cosmos;Standard;Serialization;Framework</PackageTags>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\..\..\..\LICENSE" Pack="true" Visible="false" PackagePath=""/>
		<None Include="..\..\..\..\msic\images\icon.png" Pack="true" Visible="false" PackagePath=""/>
	</ItemGroup>

</Project>